    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "1.18.9"
  },
  "dependencies": {
    "bcrypt": "3.0.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "graphql": "14.0.2",
    "jsonwebtoken": "8.4.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
